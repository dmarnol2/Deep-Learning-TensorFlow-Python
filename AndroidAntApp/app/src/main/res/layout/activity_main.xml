        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageButton
        android:id="@+id/imageButton2"

        android:layout_width="22dp"
